{
  "problemMatcher": [
    {
      "code": "E",
      "pattern": [
        {
          "regexp": "^#{0,2}\\[([a-z]*)\\](\\S*)(\\d+):(\\d+): ([a-z]+): (.*)$",
          "file": 2,
          "column": 4,
          "line": 3,
          "severity": 1,
          "message": 6
        }
      ],
      "owner": "mypy_severity"
    }
  ]
}
